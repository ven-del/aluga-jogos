{
    "functions": {
      "src/pages/api/**/*.js": {
        "runtime": "@vercel/node"
      }
    },
    "rewrites": [
      {
        "source": "/api/produtos",
        "destination": "/api/produtos.json"
      }
    ],
    "headers": [
      {
        "source": "/api/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "s-maxage=300, stale-while-revalidate"
          }
        ]
      }
    ]
  }